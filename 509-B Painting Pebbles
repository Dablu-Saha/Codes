const int mod = 1e9 + 7;
const double PI = 3.141592653589793238;
#define ll long long int
#define cint(n) ll n;cin>>n;cin.ignore();
#define ailoop(a,n) for(int i=0;i<n;i++) {cint(x);a.pb(x);}
#define loop(i,a,b) for(int i=a;i<b;i++)
#define rloop(i,a,b) for(int i=b-1;i>=a;i--)
#define all(v) v.begin(),v.end()
#define vi vector<int>
#define pii pair<int,int>
#define ff first
#define ss second
#define pb push_back
#define ppb pop_back
#define debug(func) cerr<<#func<<" : "<<func<<endl;
#define endl "\n"
#define spc " "
//int power(int a,int b) {if(b==0) return 1;if((b%2)==0) return (power(a*a,b/2))%mod;else return ((a%mod)*power(a,b-1)%mod)%mod;}
//int lcm(int a, int b) {return ((a*b)/__gcd(a,b));}


/* AUthor: DABLU SAHA*/




int main(){
    ll n,k;
    cin>>n>>k;
    vi a(n,0);
    ll mi=INTMAX_MAX,ma=0;
    loop(i,0,n){
        cin>>a[i];
        ll x=a[i];
        
    }
    ma=*max_element(all(a));
    mi=*min_element(all(a));
    string s1="";
    loop(i,0,mi+1){
        s1+="1 ";
    }
    string s2="";
    loop(i,0,mi){
        s2+="1 ";
    }

    //cout<<ma<<mi<<" max n min"<<endl;
    if((ma-(mi+1))>(k-1)){
        cout<<"NO";
    }
    else{
        cout<<"YES"<<endl;
        loop(i,0,n){
            ll z=a[i];
            if(z!=mi){
            
                cout<<s1;
                loop(j,2,z-(mi+1)+2){
                    cout<<j<<" ";
                }
            }
            else{
                cout<<s2;
            }
            cout<<endl;

            
        }
    }


} //end of main 



